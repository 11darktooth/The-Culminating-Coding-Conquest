#include <stdio.h>

#define MAX_FIB 4000000

int main(int argc, char** argv)
{
  unsigned int a = 1, b = 1, c = 1, sum = 1;

  while(c <= MAX_FIB)
  {
    if(c % 2 == 0)
      sum += c;
    c = a + b;
    a = b;
    b = c;
  }

  printf("The sum of the even-valued terms under %d in the fib. sequence is %u\n", MAX_FIB, sum); 
  
  return 0;
}
